---
# Defaults
my_profile: False
matt_programming: False
ZFS: False
pico: False
ansible_control: False
postgresql: False
nginx: False
phpfpm: False
postfix: False
www_home: False
jailhost: False

# SSH
sshd: "YES"
ansible_ssh_user: ansible
ansible_ssh_private_key_file: ~/.ssh/ansible_rsa
ssh_public_key_file:  ~/.ssh/ansible_rsa.pub

# Hostsfile
hostsfile:
  '127.0.0.1': ['localhost', 'localhost.localdomain', 'bacula']
